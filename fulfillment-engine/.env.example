# Private key for the fulfillment engine wallet (with 0x prefix)
PRIVATE_KEY=0x...

# Base Sepolia RPC URL
RPC_URL=https://sepolia.base.org

# Sector Vault contract address
# Underlying tokens and weights are fetched automatically from the vault
SECTOR_VAULT=0x629Eea75B6932bC214fF9486bcA86012483771C1

# Polling interval in seconds
POLL_INTERVAL=12

# Graceful shutdown timeout in seconds (default: 30)
# Time to wait for in-flight fulfillments to complete before forcing exit
SHUTDOWN_TIMEOUT=30

# Logging configuration
# Log level: DEBUG, INFO, WARN, ERROR (default: INFO)
LOG_LEVEL=INFO

# Log format: TEXT or JSON (default: TEXT)
# Use JSON for production to enable structured log parsing by systemd/journald
LOG_FORMAT=TEXT
